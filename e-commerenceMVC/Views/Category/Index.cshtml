@model List<Category>
<h1>Kategori Sayfası</h1>
<table class="table table-bordered table-striped">
	<thead>
		<tr>
			<th>
				Category Name
			</th>
			<th>
				Display Order
			</th>
		</tr>
	</thead>
	<tbody>
		@foreach(var obj in Model.OrderByDescending(u => u.DisplayOrder)) //sıralama yaptık display ordera göre
		{
			<tr>
			<td>@obj.Name</td>
				<td>@obj.DisplayOrder</td>
			</tr>
		}
	</tbody>
</table>
